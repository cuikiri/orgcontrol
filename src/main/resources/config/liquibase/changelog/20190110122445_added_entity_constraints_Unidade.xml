<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Unidade.
    -->
    <changeSet id="20190110122445-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="endereco_id"
                                 baseTableName="unidade"
                                 constraintName="fk_unidade_endereco_id"
                                 referencedColumnNames="id"
                                 referencedTableName="endereco"/>

        <addForeignKeyConstraint baseColumnNames="tipo_unidade_id"
                                 baseTableName="unidade"
                                 constraintName="fk_unidade_tipo_unidade_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipo_unidade"/>

        <addForeignKeyConstraint baseColumnNames="instituicao_id"
                                 baseTableName="unidade"
                                 constraintName="fk_unidade_instituicao_id"
                                 referencedColumnNames="id"
                                 referencedTableName="instituicao"/>

    </changeSet>
</databaseChangeLog>
